{
  "realm": "campus-mesh",
  "auth_url": "https://auth.campus-mesh.edu/auth/realms/campus-mesh",
  "resource": "campus-mesh-app",
  "public_client": true,
  "confidential_port": 0,
  "ssl_required": "external",
  "use_resource_role_mappings": true,
  "cors_allowed_origins": [
    "http://localhost:3000",
    "https://campus-mesh.edu"
  ],
  "cors_allowed_methods": [
    "GET",
    "POST",
    "PUT",
    "DELETE",
    "OPTIONS"
  ],
  "cors_exposed_headers": [
    "Authorization"
  ],
  "enable_cors": true,
  "turn_servers": [
    {
      "urls": "stun:stun.l.google.com:19302"
    },
    {
      "urls": "turn:your-turn-server.com:3478",
      "username": "your-username",
      "credential": "your-password"
    }
  ],
  "ice_servers": [
    {
      "urls": "stun:stun.l.google.com:19302"
    }
  ],
  "signaling_server": {
    "url": "wss://signaling.campus-mesh.edu",
    "port": 443
  },
  "media_constraints": {
    "audio": true,
    "video": {
      "width": {
        "min": 320,
        "ideal": 1280,
        "max": 1920
      },
      "height": {
        "min": 240,
        "ideal": 720,
        "max": 1080
      },
      "frameRate": {
        "ideal": 30,
        "max": 60
      }
    }
  },
  "notification_settings": {
    "vapid_public_key": "your-vapid-public-key",
    "firebase_sender_id": "your-firebase-sender-id"
  }
}